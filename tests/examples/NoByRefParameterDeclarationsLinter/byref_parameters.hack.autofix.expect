function byref_parameters(
  inout string $ref,
  float $not_ref,
  /* has comment */ inout bool /* and weird */  /* formatting */ $ref2,
  inout mixed  $ref3,
  inout mixed $ref4,
  // hello
  inout mixed$ref5,
  inout $ref6,
  // hi
  inout $ref7,
  /* foo */ inout /* $bar */ $ref8,
  int &$ref_with_default = 42,
): void {
  // not a parameter, no auto-fix, no Lint error
  $ref = &$foo;
  $ref2 =& $ref;
}
