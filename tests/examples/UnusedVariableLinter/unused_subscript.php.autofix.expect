<?hh

function subscript_expressions(): void {
  $_a = dict[];
  $b = 'b';
  $c = 'c';
  $d = 'd';
  $_a['a'] = 1;
  $_a[$b] = 5;
  $_a[$c.$d] = 10;
  $_a[1]['a']['b'] = 10;
  $_a[1]['c'][] = 10;

  return;
}

